<core:FragmentDefinition xmlns="sap.m" xmlns:layout="sap.ui.layout" xmlns:form="sap.ui.layout.form" xmlns:micro="sap.suite.ui.commons" xmlns:core="sap.ui.core">
    <FlexBox class="sapUiMediumMarginTop" visible="{genericTileModel>/sStep5}" justifyContent="Center" alignContent="Center">
        <form:SimpleForm width="45rem" title="Accounting Information" layout="ResponsiveGridLayout" labelSpanXL="8" labelSpanL="8" labelSpanM="12" columnsXL="1" columnsL="2" columnsM="1" editable="true" adjustLabelSpan="true" singleContainerFullSize="false">
            <form:content>
                <!-- <Title text="Address" level="H4" titleStyle="H4" /> -->
                <VBox>
                    <Label text="Return to Vendor" design="Bold" />
                    <ComboBox width="100%" />

                    <layoutData>
                        <layout:GridData span="XL12 L12 M12 S12" linebreak="true" />
                    </layoutData>
                </VBox>
                <VBox>
                    <Label text="No Return Margin %" design="Bold" />
                    <Input />
                    <layoutData>
                        <layout:GridData span="XL12 L12 M12 S12" linebreak="true" />
                    </layoutData>
                </VBox>
                <VBox>
                    <Label text="Vendor Claim %" design="Bold" />
                    <Input />
                    <layoutData>
                        <layout:GridData span="XL12 L12 M12 S12" linebreak="true" />
                    </layoutData>
                </VBox>
                <VBox>
                    <Label text="Credit Period" design="Bold" />
                    <ComboBox width="100%" />

                    <layoutData>
                        <layout:GridData span="XL12 L12 M12 S12" linebreak="true" />
                    </layoutData>
                </VBox>
                <VBox>
                    <Label text="Refundable Deposit Fee" design="Bold" />
                    <Input />
                    <layoutData>
                        <layout:GridData span="XL12 L12 M12 S12" linebreak="true" />
                    </layoutData>
                </VBox>
                <VBox>
                    <Label text="Shipping Conditions" design="Bold" />
                    <ComboBox width="100%" />

                    <layoutData>
                        <layout:GridData span="XL12 L12 M12 S12" linebreak="true" />
                    </layoutData>
                </VBox>
                <VBox>
                    <Label text="Reconcialition Accounts" design="Bold" />
                    <ComboBox width="100%" />
                    <layoutData>
                        <layout:GridData span="XL12 L12 M12 S12" linebreak="true" />
                    </layoutData>
                </VBox>
                <VBox>
                    <Label text="Sort Key" design="Bold" />
                    <ComboBox width="100%" value="{genericTileModel>/Email}" />
                    <layoutData>
                        <layout:GridData span="XL12 L12 M12 S12" linebreak="true" />
                    </layoutData>
                </VBox>
                <VBox>
                    <Label text="Reconcialition Accounts" design="Bold" />
                    <ComboBox width="100%" />
                    <layoutData>
                        <layout:GridData span="XL12 L12 M12 S12" linebreak="true" />
                    </layoutData>
                </VBox>
                <VBox>
                    <Button text="Proceed to Payment information" press="onfnnextpress6" width="100%" class="button1" />
                    <layoutData>
                        <layout:GridData span="XL12 L12 M12 S12" linebreak="false" />
                    </layoutData>
                </VBox>
            </form:content>
        </form:SimpleForm>
    </FlexBox>
</core:FragmentDefinition>
